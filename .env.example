# Kadim Savaşlar - Environment Variables
# Bu dosyayı .env.local olarak kopyalayın ve değerleri doldurun

# Socket Server URL (Production için)
# Örnek: https://kadim-savaslar-server.up.railway.app
VITE_SOCKET_URL=

# API Base URL (Production için)
# Örnek: https://kadim-savaslar-api.up.railway.app
VITE_API_URL=

# MongoDB Connection (Backend için)
MONGODB_URI=

# JWT Secret (Backend için)
JWT_SECRET=your-super-secret-jwt-key

# Node Environment
NODE_ENV=development
